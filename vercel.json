{
	"version": 2,
	"rewrites": [
	  {
		"source": "/old-path",
		"destination": "/new-path"
	  }
	],
	"redirects": [
	  {
		"source": "/old-page",
		"destination": "/new-page",
		"permanent": true
	  }
	],
	"headers": [
	  {
		"source": "/api/(.*)",
		"headers": [
		  { "key": "Access-Control-Allow-Origin", "value": "*" }
		]
	  }
	],
	"trailingSlash": false
  }
  